<h2 class="text-center m-5">メッセージ中のクリエイター</h2>
  <div class ="row">
  <% @anotherEntries.each do |room| %>
    <div class="col-3 text-center">
      <%= link_to chat_path(room.room.id) do %>
        <%= attachment_image_tag room.user, :image, format: 'jpg', size: "45x45", fallback: "no_image.png", class: "rounded-circle" %>
        <%= room.user.name %><br>
      <% end %>
    </div>
  <% end %>
  </div>